package com.study.Todo_list.entity;

import com.study.Todo_list.Dto.insertTodoListReqDto;
import com.study.Todo_list.Dto.insertTodoListRespDto;

import lombok.Builder;
import lombok.Data;

@Builder
@Data

public class TodoList {
	private int TodoListId;
	private String TotoListContext;
	
	public insertTodoListRespDto toDto() {
		return insertTodoListReqDto.builder()
								.TotoListContext(TotoListContext)
								.build();
	}
}
